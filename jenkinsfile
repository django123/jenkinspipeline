pipeline {
    agent any


    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('BUILD') {
            steps {
                echo 'Developpement'
            }
        }
            stage('TEST') {
            steps {
                echo 'TESTING'
            }
        }
        stage('DEPLOY') {
            steps {
                echo 'Mise en prod!!'
            }
        }
    }

}
